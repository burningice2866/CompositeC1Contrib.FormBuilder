<configuration>
  <Composite.C1Console.Elements.Plugins.ElementProviderConfiguration>
    <ElementProviderPlugins>
      <add type="CompositeC1Contrib.FormBuilder.Dynamic.C1Console.ElementProvider.FormBuilderElementProvider, CompositeC1Contrib.FormBuilder.Dynamic" name="FormBuilder.Dynamic" />

      <add name="VirtualElementProvider">
        <Perspectives>
          <add name="ContentPerspective">
            <Elements>
              <add providerName="FormBuilder.Dynamic" name="FormBuilder.Dynamic" type="Composite.Plugins.Elements.ElementProviders.VirtualElementProvider.AttachProviderVirtualElement, Composite" />
            </Elements>
          </add>
        </Perspectives>
      </add>
    </ElementProviderPlugins>
  </Composite.C1Console.Elements.Plugins.ElementProviderConfiguration>

  <Composite.C1Console.Forms.Plugins.ProducerMediatorConfiguration>
    <Mediators>
      <add type="Composite.C1Console.Forms.StandardProducerMediators.UiControlProducerMediator, Composite" name="https://bitbucket.org/burningice/compositec1contrib.formbuilder/1.0" />
    </Mediators>
  </Composite.C1Console.Forms.Plugins.ProducerMediatorConfiguration>

  <Composite.C1Console.Forms.Plugins.UiControlFactoryConfiguration>
    <Channels>
      <Channel>
        <Namespaces>
          <Namespace name="https://bitbucket.org/burningice/compositec1contrib.formbuilder/1.0">
            <Factories>
              <add userControlVirtualPath="~/Composite/InstalledPackages/CompositeC1Contrib.FormBuilder.Dynamic/FunctionSelector.ascx" cacheCompiledUserControlType="false" type="Composite.Plugins.Forms.WebChannel.UiControlFactories.UserControlBasedUiControlFactory, Composite" name="FunctionSelector" />
            </Factories>
          </Namespace>
        </Namespaces>
      </Channel>
    </Channels>
  </Composite.C1Console.Forms.Plugins.UiControlFactoryConfiguration>

  <Composite.Functions.Plugins.FunctionProviderConfiguration>
    <FunctionProviderPlugins>
      <add type="CompositeC1Contrib.FormBuilder.FunctionProviders.DynamicFormFunctionProvider, CompositeC1Contrib.FormBuilder.Dynamic" name="FormBuilder.Dynamic" />
    </FunctionProviderPlugins>
  </Composite.Functions.Plugins.FunctionProviderConfiguration>
</configuration>