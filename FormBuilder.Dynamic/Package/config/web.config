<?xml version="1.0"?>
<configuration>
  <compositeC1Contrib>
    <formBuilder>
      <dynamic type="CompositeC1Contrib.FormBuilder.Dynamic.Configuration.DynamicFormBuilderConfiguration, CompositeC1Contrib.FormBuilder.Dynamic">
        <inputElementHandlers>
          <add name="Textbox" type="CompositeC1Contrib.FormBuilder.Web.UI.TextboxInputElement, CompositeC1Contrib.FormBuilder" />
          <add name="Password" type="CompositeC1Contrib.FormBuilder.Web.UI.PasswordInputElement, CompositeC1Contrib.FormBuilder" />
          <add name="Textarea" type="CompositeC1Contrib.FormBuilder.Web.UI.TextAreaInputElement, CompositeC1Contrib.FormBuilder" settingsHandler="CompositeC1Contrib.FormBuilder.Dynamic.TextAreaSettings, CompositeC1Contrib.FormBuilder.Dynamic" />
          <add name="Checkbox" type="CompositeC1Contrib.FormBuilder.Web.UI.CheckboxInputElement, CompositeC1Contrib.FormBuilder" />
          <add name="Dropdown" type="CompositeC1Contrib.FormBuilder.Web.UI.DropdownInputElement, CompositeC1Contrib.FormBuilder" />
          <add name="File upload" type="CompositeC1Contrib.FormBuilder.Web.UI.FileuploadInputElement, CompositeC1Contrib.FormBuilder" />
          <add name="Radio buttons" type="CompositeC1Contrib.FormBuilder.Web.UI.RadioButtonInputElement, CompositeC1Contrib.FormBuilder" />
        </inputElementHandlers>
      </dynamic>
    </formBuilder>
  </compositeC1Contrib>
</configuration>